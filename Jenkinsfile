pipeline {
    agent any 
    stages {
      stage('Build'){
        steps {
          echo "Build step"
          sleep 4
        }
      
      
      }
        stage('test '){
        steps {
          echo "test step"
          sleep 5
        }
      
      
      }
        stage('deploy'){
        steps {
          echo "deploy war file"
          sleep 10
        }
      
      
      }
    
    }

}

